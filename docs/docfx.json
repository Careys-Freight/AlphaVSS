{
   "metadata": [
      {
         "src": [
            {
               "src": "..",
               "files": [
                  "src/AlphaVSS.Common/AlphaVSS.Common.csproj"
               ]
            }
         ],
         "dest": "api",
         "filter": "filterConfig.yml",
         "disableGitFeatures": false,
         "disableDefaultFilter": false
      }
   ],
   "build": {
      "content": [
         {
            "files": [
               "api/**.yml",
               "api/index.md"
            ]
         },
         {
            "files": [
               "articles/**.md",
               "articles/**/toc.yml",
               "toc.yml",
               "*.md"
            ]
         },
         {
            "files": "ChangeLog.md",
            "src": ".."
         }
      ],
      "xref": [
         "external_xrefs.yml"
      ],
      "xrefservice": [
         "https://xref.docs.microsoft.com/query?uid={uid}"
      ],
      "resource": [
         {
            "files": [
               "images/**"
            ]
         }
      ],
      "overwrite": [
         {
            "files": [
               "apidoc/**.md"
            ],
            "exclude": [
               "obj/**",
               "_site/**"
            ]
         }
      ],
      "dest": "../artifacts/docs",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [         
         "default",
         "templates/alphavss"        
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
   }
}
